  "homepage": "https://github.com/Strobotti/php-jwk",
  "authors": [
    {
      "name": "Juha Jantunen",
      "email": "juha@strobotti.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "phpseclib/phpseclib": "^2.0"
